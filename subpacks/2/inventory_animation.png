    "$overlay_texture": "textures/ui/inventory_overlay_old"
  },

  "inv_bg_base": {
    "type": "panel",
    "size": [ 176, 166 ],
    "$flipbook_res": [ 352, 332 ],
    "$overlay_texture": "textures/ui/inventory_overlay",
    "controls": [
      {
        "overlay": {
          "type": "image",
          "size": [ 176, 166 ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "texture": "$overlay_texture",
          "layer": 2
        }
      },
      {
        "inventory_texture": {
          "type": "image",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "uv_size": "$flipbook_res",
          "texture": "textures/animated_gui/_Zenitsu_", 
          "uv": "@CrisXolt_anm_inv_bg_base.01",
          "size": [ 176, 166 ]
        }
      }
    ]
  },

