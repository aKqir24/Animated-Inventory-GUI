{
  "namespace": "CrisXolt_anm_inv_bg_base",

  "inv_bg_base_old@CrisXolt_anm_inv_bg_base.inv_bg_base": {
    "$overlay_texture": "textures/ui/inventory_overlay_old"
  },

  "inv_bg_base": {
    "type": "panel",
    "size": [ 176, 166 ],
    "$flipbook_res": [ 512, 492 ],
    "$overlay_texture": "textures/ui/inventory_overlay",
    "controls": [
      {
        "overlay": {
          "type": "image",
          "size": [ 176, 166 ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "texture": "$overlay_texture",
          "layer": 2
        }
      },
      {
        "inventory_texture": {
          "type": "image",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "uv_size": "$flipbook_res",
          "texture": "textures/animated_gui/Finger_Spin", 
          "uv": "@CrisXolt_anm_inv_bg_base.01",
          "size": [ 176, 166 ]
        }
      }
    ]
  },

  /*//// Inventory Bar Frames ////*/

  "inventory_uv_base@CrisXolt_anm_inventory_bg.uv_base": {
    "duration": 0.11,
    "variables": [
      {
        "$next_frame": "@CrisXolt_anm_inv_bg_base.01"
      }
    ]
  },
    
  "01@CrisXolt_anm_inv_bg_base.inventory_uv_base": { "$uv_frame": [ 0, 0     ], "$next_frame": "@CrisXolt_anm_inv_bg_base.02" },
  "02@CrisXolt_anm_inv_bg_base.inventory_uv_base": { "$uv_frame": [ 0, 492   ], "$next_frame": "@CrisXolt_anm_inv_bg_base.03" },
  "03@CrisXolt_anm_inv_bg_base.inventory_uv_base": { "$uv_frame": [ 0, 984   ], "$next_frame": "@CrisXolt_anm_inv_bg_base.04" },
  "04@CrisXolt_anm_inv_bg_base.inventory_uv_base": { "$uv_frame": [ 0, 1476  ], "$next_frame": "@CrisXolt_anm_inv_bg_base.05" },
  "05@CrisXolt_anm_inv_bg_base.inventory_uv_base": { "$uv_frame": [ 0, 1968  ], "$next_frame": "@CrisXolt_anm_inv_bg_base.01" }

}